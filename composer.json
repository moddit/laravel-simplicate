{
    "name": "moddit/laravel-simplicate",
    "description": "Laravel Simplicate API Client",
    "keywords": [ "laravel", "simplicate", "api", "client", "laravel9" ],
    "homepage": "https://github.com/moddit/laravel-simplicate",
    "require": {
        "php": "^8.1.2",
        "guzzlehttp/guzzle": "^7.4"
    },
    "autoload": {
        "psr-4": {
            "Czim\\Simplicate\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Czim\\Simplicate\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Czim\\Simplicate\\Providers\\SimplicateServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
